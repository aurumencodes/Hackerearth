/*
(
Aim of the problem: 
Input format:
First line contains integer N.
Second line contains string S.

Output format:
First line should contain N x 2.
Second line should contain the same string S.
)
*/
#include<bits/stdc++.h>
using namespace std;

int main()
{
	int n;
	string s;
	cin>>n;
	cin>>s;
	cout<<n*2<<endl;
	cout<<s;
	return 0;
}
